  "require": {
    "guzzlehttp/psr7": "^2.4",
    "laminas/laminas-httphandlerrunner": "^2.1",
    "laminas/laminas-diactoros": "^2.13",
    "laminas/laminas-stratigility": "^3.8"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.5",
